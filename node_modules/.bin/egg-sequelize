#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../_egg-sequelize@3.1.5@egg-sequelize/bin/egg-sequelize.js" "$@"
  ret=$?
else 
  node  "$basedir/../_egg-sequelize@3.1.5@egg-sequelize/bin/egg-sequelize.js" "$@"
  ret=$?
fi
exit $ret
